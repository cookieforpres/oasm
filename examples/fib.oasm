!entry _start

fib:
    mov r0, 0
    mov r1, 1
    sub r3, r3, 1

fib_loop:
    add r0, r0, r1
    mov r2, r0

    mov r0, r1
    mov r1, r2

    sub r3, r3, 1
    cmp r3, 0
    jne fib_loop

    ret

_start:
    mov sp, $0800
    
    mov r3, 32          ; nth
    call fib

    str $00, r2
    mov r0, $00
    mov r1, $00
    int 10

    mov r3, r0
    add r3, r3, i64($00)
    str r3, 10
    add r0, r0, i64(1)
    mov r3, r0

    mov r0, 1
    mov r1, r1
    mov r2, r3
    int 2

    halt